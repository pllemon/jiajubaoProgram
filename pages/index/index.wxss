page{
  background: #fff;
}

.nav-section{
  display: flex;
  flex: 1;
  padding: 6rpx;
}
.nav-section .rows{
  flex: 1;
  display: flex;
  flex-direction: column;
}
.nav-section .rows .link {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 38rpx;
  color: #fff;
  margin: 6rpx;
  border-radius: 10rpx;
}
.nav-section .rows .link image{
  width: 70rpx;
  height: 70rpx;
  margin-bottom: 20rpx;
  display: block;
}