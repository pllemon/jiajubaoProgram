page{
    background: #fff;
}
.share-view{
    padding: 30rpx;
}
.share-view image{
    width: 100%;
    height: 100%;
}

.share-btn{
    width: 140rpx;
    height: 140rpx;
    border-radius: 50%;
    background: -webkit-linear-gradient(45deg, #7889f1, #4d5ef4);
    font-size: 40rpx;
    color: #fff;
    margin: 30rpx auto;
    margin-top: 0;
}
.share-btn icon{
    font-size: 50rpx;
}